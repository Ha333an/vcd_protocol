{
  "name": "react-example",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --port=3000 --host=0.0.0.0",
    "build:web": "vite build",
    "build:extension": "esbuild src/extension.ts --bundle --platform=node --target=node16 --external:vscode --outfile=out/extension.js",
    "build": "npm run build:web",
    "vscode:prepublish": "npm run build:extension && npm run build:web",
    "preview": "vite preview",
    "clean": "rm -rf dist out",
    "lint": "tsc --noEmit",
    "package": "vsce package"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/d3": "^7.4.3",
    "@vitejs/plugin-react": "^5.0.4",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.5.0",
    "vcd-parser": "^1.0.1",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/vscode": "^1.79.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.18.0",
    "@emnapi/core": "^1.8.1",
    "@emnapi/runtime": "^1.8.1",
    "@emnapi/wasi-threads": "^1.1.0",
    "@napi-rs/wasm-runtime": "^1.1.1",
    "@tybys/wasm-util": "^0.10.1",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vsce": "^2.0.1"
  },
  "publisher": "HassanEbrahimi",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ha333an/vcd_protocol.git"
  },
  "bugs": {
    "url": "https://github.com/Ha333an/vcd_protocol/issues"
  },
  "homepage": "https://github.com/Ha333an/vcd_protocol#readme",
  "engines": {
    "vscode": "^1.79.0"
  },
  "main": "out/extension.js",
  "activationEvents": [
    "onCommand:vcdProtocol.openViewer",
    "onLanguage:vcd"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vcd",
        "extensions": [".vcd"],
        "aliases": ["VCD"]
      }
    ],
    "commands": [
      {
        "command": "vcdProtocol.openViewer",
        "title": "Open VCD Viewer"
      }
    ]
  }
}
